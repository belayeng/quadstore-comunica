{
  "name": "quadstore-comunica",
  "version": "2.0.0",
  "description": "This library lets you use the Comunica query engine with quadstore. It is not meant to be used on its own but rather as a dependency of quadstore itself.",
  "main": "index.bundle.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-engine": "comunica-compile-config config/config-quadstore.json > engine.js",
    "build-bundle": "webpack --config webpack.config.js",
    "build-bundle-rename-vars": "mv index.bundle.js b.js && sed -e 's/__webpack_/__cmncwpk_/g' b.js > index.bundle.js && rm b.js",
    "build": "npm run build-engine && npm run build-bundle && npm run build-bundle-rename-vars",
    "analyze-bundle": "webpack --config webpack.config.js --profile --json > webpack.stats.json && webpack-bundle-analyzer webpack.stats.json",
    "publish:alpha": "npm publish --tag alpha",
    "publish:beta": "npm publish --tag beta"
  },
  "homepage": "https://github.com/belayeng/quadstore-comunica",
  "bugs": "https://github.com/belayeng/quadstore/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/belayeng/quadstore-comunica.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@comunica/query-sparql": "^2.0.0",
    "@comunica/types": "^2.0.0",
    "json-loader": "^0.5.7",
    "webpack": "^5.59.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "peerDependencies": {
    "asynciterator": "^3.2.1",
    "rdf-data-factory": "^1.1.0"
  },
  "files": [
    "index.d.ts",
    "index.bundle.js"
  ],
  "lsd:module": true
}
